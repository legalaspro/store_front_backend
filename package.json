{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "nodemon src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "create:testdb": "npx db-migrate db:create store_front_test",
    "drop:testdb": "npx db-migrate db:drop store_front_test",
    "test:run": "ENV=test npx ts-node ./jasmine-setup.js",
    "test:full": "ENV=test npx db-migrate up -e test && npm run test:run ; npx db-migrate down -e test -c 100",
    "tsc": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "1.20.2",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.3.0",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/express": "4.17.17",
    "@types/jasmine": "^4.3.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/pg": "8.10.2",
    "@types/supertest": "^2.0.12",
    "jasmine": "^5.0.2",
    "jasmine-spec-reporter": "7.0.0",
    "jasmine-ts": "0.4.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "6.0.4",
    "typescript": "^5.1.3"
  }
}
